&pinctrl {
	//pinctrl-names = "default";
	//pinctrl-0 = <&bklt_en &lvds_vdd_en &wdt_pwn &wdt_en &gpio8_a0 &gpio8_a1 &gpio8_a2 &gpio8_a3>;

	//used for init some gpio
	init-gpios =</*&gpio0 GPIO_C2 GPIO_ACTIVE_HIGH */
			/*&gpio7 GPIO_B7 GPIO_ACTIVE_LOW */>;

	power_pin {
		sleep_btn:gpio2-a2 {
			rockchip,pins = <GPIO2_A2>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		power_btn:gpio2-a5 {
			rockchip,pins = <GPIO2_A5>;
			rockchip,pull = <VALUE_PULL_UP>;
		};
	};

	c8051_pin {
		wdt_pwn:gpio2-a4 {
			rockchip,pins = <GPIO2_A4>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		wdt_en:gpio2-a3 {
			rockchip,pins = <GPIO2_A3>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};
	};

	//could add other pinctrl definition such as gpio
	//lvds power gpio pinmux
	lvds_power_pin {
		bklt_en:gpio1_d3 {
			rockchip,pins = <GPIO1_D3>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		lvds_vdd_en:gpio1_d2 {
			rockchip,pins = <GPIO1_D2>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};
	};

	//minipcie power gpio pinmux
	minipcie_pin {
		minipcie_reset:gpio2_a7 {
			rockchip,pins = <GPIO2_A7>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		minipcie_dis:gpio2_b1 {
			rockchip,pins = <GPIO2_B1>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};
	};

	//M.2 power gpio pinmux
	m2_pin {
		m2_dis1:gpio5-c3 {
			rockchip,pins = <GPIO5_C3>;
			rockchip,pull = <VALUE_PULL_UP>;
		};

		m2_dis2:gpio5-c1 {
			rockchip,pins = <GPIO5_C1>;
			rockchip,pull = <VALUE_PULL_UP>;
		};

		m2_poweren:gpio4-d4 {
			rockchip,pins = <GPIO4_D4>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		m2_reset:gpio5-c2 {
			rockchip,pins = <GPIO5_C2>;
			rockchip,pull = <VALUE_PULL_UP>;
		};

		m2_sdio_wake:gpio4-d2 {
			rockchip,pins = <GPIO4_D2>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		m2_int:gpio4-d6 {
			rockchip,pins = <GPIO4_D6>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};
	};

	//normal user gpio pinmux
	normal_gpio_pin {
		gpio8_a0:gpio8-a0 {
			rockchip,pins = <GPIO8_A0>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		gpio8_a1:gpio8-a1 {
			rockchip,pins = <GPIO8_A1>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		gpio8_a2:gpio8-a2 {
			rockchip,pins = <GPIO8_A2>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		gpio8_a3:gpio8-a3 {
			rockchip,pins = <GPIO8_A3>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		/*default gpio,so donot config pinmux
		gpio7_a3:gpio3-a3 {
			rockchip,pins = <GPIO7_A3>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		gpio7_a4:gpio3-a4 {
			rockchip,pins = <GPIO7_A4>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		gpio7_a5:gpio3-a5 {
			rockchip,pins = <GPIO7_A5>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		gpio7_c5:gpio3-c5 {
			rockchip,pins = <GPIO3_C5>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};
		*/
	};

	//EDP config
	edp_pin {
		edp_hdp:edp_hdp {
			rockchip,pins = <EDP_HOTPLUG>;
			rockchip,pull = <VALUE_PULL_DISABLE>;
			rockchip,drive = <VALUE_DRV_DEFAULT>;
		};
	};

	//I2S mute config
	i2s_mute_pin {
		i2s_shutdown:gpio6-a5 {
			rockchip,pins = <GPIO6_A5>;
			rockchip,pull = <VALUE_PULL_DISABLE>;
		};

		i2s_mute:gpio6-a6 {
			rockchip,pins = <GPIO6_A6>;
			rockchip,pull = <VALUE_PULL_DISABLE>;
		};
	};

	uart4_sel_pin {
		uart4_sel:gpio5-c0 {
			rockchip,pins = <GPIO5_C0>;
			rockchip,pull = <VALUE_PULL_DISABLE>;
		};
	};

	reserve_gpio_pin {
		uart3_rtsn:gpio2-b2 {
			rockchip,pins = <GPIO2_B2>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		c8051_reserve:gpio2-a6 {
			rockchip,pins = <GPIO2_A6>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		m2_alert:gpio2-b0 {
			rockchip,pins = <GPIO2_B0>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		m2_wake:gpio6-a7 {
			rockchip,pins = <GPIO6_A7>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};

		m2_reserve:gpio6-b3 {
			rockchip,pins = <GPIO6_B3>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};
	};

	usb_oc {
		/*default gpio,so donot config pinmux
		usb12_oc:GPIO7-A2 {
			rockchip,pins = <GPIO7_A2>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};
		*/
		usb34_oc:GPIO1-D0 {
			rockchip,pins = <GPIO1_D0>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};
		usb5_oc:GPIO1-D1 {
			rockchip,pins = <GPIO1_D1>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};
	};

	otg {
		/*default gpio,so donot config pinmux
		usb_int:GPIO0-C2 {
			rockchip,pins = <GPIO0_C2>;
			rockchip,pull = <VALUE_PULL_DOWN>;
		};
		*/
	};

	// gmac drive strength
	gpio4_gmac {
		mac_clk: mac-clk {
			rockchip,drive = <VALUE_DRV_12MA>;
		};

		mac_txpins: mac-txpins {
			rockchip,drive = <VALUE_DRV_12MA>;
		};

		mac_rxpins: mac-rxpins {
			rockchip,drive = <VALUE_DRV_12MA>;
		};

		mac_crs: mac-crs {
			rockchip,drive = <VALUE_DRV_12MA>;
		};

		mac_mdpins: mac-mdpins {
			rockchip,drive = <VALUE_DRV_12MA>;
		};
	};
};

