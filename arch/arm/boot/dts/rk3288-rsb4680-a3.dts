		rockchip,iommu-enabled = <0>;
		reg = <0xff930000 0x10000>;
		interrupts = <GIC_SPI 15 IRQ_TYPE_LEVEL_HIGH>;
		pinctrl-names = "default", "gpio";
		pinctrl-0 = <&lcdc0_gpio>;
		pinctrl-1 = <&lcdc0_gpio>;
		status = "disabled";
		clocks = <&clk_gates15 5>, <&dclk_lcdc0>, <&clk_gates15 6>, <&pd_vop0>;
		clock-names = "aclk_lcdc", "dclk_lcdc", "hclk_lcdc", "pd_lcdc";
	};
};

&gmac {
//	power_ctl_by = "gpio";	//"gpio" "pmu"
//	power-gpio = <&gpio4 GPIO_A2 GPIO_ACTIVE_HIGH>;
//	power-pmu = "act_ldo"
	reset-gpio = <&gpio4 GPIO_A7 GPIO_ACTIVE_LOW>;
	phy-mode = "rgmii";
	clock_in_out = "input";//"input";
	tx_delay = <0x0>;//mac tx delay
	rx_delay = <0x0>;//mac rx delay
	gmac_v18sel = <1>;//rgmii 1.8v/3.3v voltage
	phy_reg_config = <//address mask value
						0x0014 0x200 0x200
						0x0018 0xfff 0x65b
						0x001e 0x200 0x200
						0x0032 0x3 0x3
						0x0086 0xff 0x80
						0x0170 0x1f00 0x0000
					>;
	status = "okay";
};

&pinctrl {
	//pinctrl-names = "default";
	//pinctrl-0 = <&bklt_en &lvds_vdd_en &wdt_pwn &wdt_en &gpio8_a0 &gpio8_a1 &gpio8_a2 &gpio8_a3>;

	//used for init some gpio
	init-gpios =</*&gpio0 GPIO_C2 GPIO_ACTIVE_HIGH */
			/*&gpio7 GPIO_B7 GPIO_ACTIVE_LOW */>;

	//could add other pinctrl definition such as gpio

	uart3_rts_pin {
		uart3_rts_gpio:gpio7-b2 {
			rockchip,pins = <GPIO7_B2>;
			rockchip,pull = <VALUE_PULL_DISABLE>;
		};
	};

	uart4_rts_pin {
		uart4_rts_gpio:gpio5-b5 {
			rockchip,pins = <GPIO5_B5>;
			rockchip,pull = <VALUE_PULL_DISABLE>;
		};
	};

	// gmac drive strength
	gpio4_gmac {
		mac_clk: mac-clk {
			rockchip,drive = <VALUE_DRV_12MA>;
		};

		mac_txpins: mac-txpins {
			rockchip,drive = <VALUE_DRV_12MA>;
		};

		mac_rxpins: mac-rxpins {
			rockchip,drive = <VALUE_DRV_12MA>;
		};

		mac_crs: mac-crs {
			rockchip,drive = <VALUE_DRV_12MA>;
		};

		mac_mdpins: mac-mdpins {
			rockchip,drive = <VALUE_DRV_12MA>;
		};
	};

	// sdmmc0 drive strength
	gpio6_sdmmc0 {
		sdmmc0_clk: sdmmc0-clk {
			rockchip,drive = <VALUE_DRV_8MA>;
		};

		sdmmc0_cmd: sdmmc0-cmd {
			rockchip,drive = <VALUE_DRV_8MA>;
		};

		sdmmc0_dectn: sdmmc0-dectn{
			rockchip,drive = <VALUE_DRV_8MA>;
		};

		sdmmc0_bus1: sdmmc0-bus-width1 {
			rockchip,drive = <VALUE_DRV_8MA>;
		};

		sdmmc0_bus4: sdmmc0-bus-width4 {
			rockchip,drive = <VALUE_DRV_8MA>;
		};
	};
};

