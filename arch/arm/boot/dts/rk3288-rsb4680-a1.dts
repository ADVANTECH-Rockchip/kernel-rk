			gpios = <&gpio2 GPIO_A2 GPIO_ACTIVE_HIGH>;
			linux,code = <116>;
			label = "power";
			gpio-key,wakeup;
		};

		power-key {
			gpios = <&gpio2 GPIO_A5 GPIO_ACTIVE_LOW>;
			linux,code = <116>;
			label = "power";
